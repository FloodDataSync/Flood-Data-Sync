type Location = 
 record {
   accuracy: float64;
   latitude: float64;
   longitude: float64;
 };
type FloodReport = 
 record {
   description: text;
   floodType: text;
   imageData: vec nat8;
   location: Location;
   timestamp: text;
 };
service : {
  getAllReports: () -> (vec FloodReport) query;
  submitReport: (report: FloodReport) -> (text);
}
